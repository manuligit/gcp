{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
      "react/jsx-one-expression-per-line": "off",
      "no-console": 1,
      "class-methods-use-this": 0
    },
    "env": {
      "browser": true,
      "node": true
    }
  }